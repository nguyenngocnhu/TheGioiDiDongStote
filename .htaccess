RewriteEngine On  

RewriteRule "index.html" "index.php"

RewriteRule "cart_view.html" "cart_view.php"

RewriteRule "contact.html" "contact.php"

RewriteRule "profile.html" "profile.php"

RewriteRule "logout.html" "logout.php"

RewriteRule "login.html" "login.php"

RewriteRule "signup.html" "signup.php"

RewriteRule "search.html" "search.php"

RewriteRule "verify.html" "verify.php"

RewriteRule "password_forgot.html" "password_forgot.php"

RewriteRule "register.html" "register.php"

RewriteRule "^product/([a-zA-Z0-9_-]+).html$" "product.php?product=$1"

RewriteRule "^category/([a-zA-Z0-9_-]+).html$" "category.php?category=$1"